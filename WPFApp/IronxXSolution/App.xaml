<Application x:Class="IronxXSolution.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:shared="clr-namespace:Library.Functions;assembly=Library"
             StartupUri="PasswordWindow.xaml">
    <Application.Resources>
        <ResourceDictionary >
            <shared:DateToggle x:Key="DateToggle"/>
            <shared:DateColors x:Key="DateColors"/>
            <shared:MultiValues x:Key="MultiValues"/>
            <shared:GameNameFromID x:Key="GameNameFromID"/>
            <shared:PCNameFromID x:Key="PCNameFromID"/>
            <shared:PCColorFromID x:Key="PCColorFromID"/>
            <shared:ProductTypeFromID x:Key="ProductTypeFromID"/>
            <shared:AdminNameFromID x:Key="AdminNameFromID"/>
            
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/StylesResources;component/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>

        </ResourceDictionary>
    </Application.Resources>
</Application>
